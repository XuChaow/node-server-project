{
  "name": "node-server",
  "version": "0.0.1",
  "description": "基于node.js技术栈的服务器端项目框架示例",
  "author": "super_xp@126.com",
  "keywords": [
    "node.js",
    "express",
    "restful api",
    "node-frameworek"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoping6688/node-server-project"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev",
    "dev": "NODE_ENV=development supervisor -w server,app.js ./bin/www",
    "prod": "pm2 startOrRestart ./config/pm2.js --env production",
    "stop": "pm2 stop ./bin/www",
    "reload": "pm2 reload ./bin/www",
    "debug": "node-debug ./bin/www",
    "live": "gulp browser-sync",
    "test": "./node_modules/mocha/bin/mocha",
    "lint": "./node_modules/.bin/eslint app.js server",
    "update": "ncu -u -a"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "amqplib-rpc": "^2.0.4",
    "blueimp-md5": "^2.7.0",
    "body-parser": "~1.17.1",
    "bunnymq": "^2.2.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.1",
    "debug": "~2.6.2",
    "express": "~4.15.2",
    "express-jwt": "^5.1.0",
    "file-stream-rotator": "0.1.0",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.8.6",
    "mongoosedao": "^1.0.13",
    "morgan": "~1.8.1",
    "mount-routes": "^1.0.6",
    "mysql": "^2.13.0",
    "mysql2": "^1.2.0",
    "pg": "^6.1.4",
    "pg-hstore": "^2.3.2",
    "redis": "^2.7.0",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "sequelize": "^3.30.2",
    "serve-favicon": "~2.4.1",
    "sqlite3": "^3.1.8",
    "thrift": "^0.10.0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "if-env": "^1.0.0",
    "mocha": "^3.2.0",
    "pm2": "^2.4.2",
    "supertest": "^3.0.0",
    "supervisor": "^0.12.0"
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 3.10.0"
  }
}
